{
  "name": "devtracker",
  "version": "1.0.0",
  "description": "Task Management System - 100 Days of Code",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:build": "tsc && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "demo:analysis": "ts-node scripts/demo-analysis.ts",
    "demo:testing": "ts-node src/demo-testing.ts",
    "docker:build": "docker build -t devtracker:latest .",
    "docker:run": "docker run -p 3000:3000 devtracker:latest",
    "docker:compose": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f k8s/",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "health:check": "bash scripts/health-check.sh",
    "lint": "echo 'Linting not configured yet'",
    "monitoring:up": "docker-compose up prometheus grafana -d",
    "monitoring:down": "docker-compose down prometheus grafana",
    "load:test": "bash scripts/load-test.sh basic",
    "load:stress": "bash scripts/load-test.sh stress",
    "benchmark": "curl -X POST http://localhost:3000/api/benchmark/test",
    "migrate:up": "ts-node scripts/migrate.ts up",
    "migrate:down": "ts-node scripts/migrate.ts down",
    "migrate:status": "ts-node scripts/migrate.ts status",
    "migration:create": "bash scripts/create-migration.sh",
    "version:info": "curl http://localhost:3000/api/versions",
    "version:deprecate": "echo 'Use API to deprecate versions'"
  },
  "keywords": [
    "typescript",
    "task-management",
    "100daysofcode"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/consul": "^2.0.0",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.3",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/exporter-jaeger": "^2.2.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "consul": "^2.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-openapi-validator": "^5.6.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.12.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "openapi-types": "^12.1.3",
    "rate-limiter-flexible": "^9.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  }
}
